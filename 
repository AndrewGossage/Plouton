#pragma once
typedef enum {
    NUMBER,
    FUNCTION,
    START,
    END,
} TokenTag;

typedef enum {
    ADD,
    SUB,
    SCOPE,
    SETX,
    SETY,

} Fn;

typedef union {
    float number;
    char *string;
    int active;
    Fn fn;
} TokenPayload;

typedef struct {
    TokenTag tag;
    TokenPayload val;
} Token;

typedef struct {
    Token *list;
    int len;
    int cap;
} TokenV;

TokenV *create_TokenV(int cap); 

int push_TokenV(TokenV *u, Token x); 

void free_TokenV(TokenV *vec); 

